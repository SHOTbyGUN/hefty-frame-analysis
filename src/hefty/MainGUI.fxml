<BorderPane fx:id="rootBorderPane" minHeight="-Infinity" minWidth="-Infinity" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hefty.MainGUIController">
<bottom><VBox fx:id="progressVBox">
</VBox>
</bottom>
<center><TabPane fx:id="rootTabPane" tabClosingPolicy="ALL_TABS" xmlns:fx="http://javafx.com/fxml">
